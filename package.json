{
  "name": "scrim-platform-server",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint . --ext .ts",
    "db:generate": "pnpm exec drizzle-kit generate",
    "db:migrate": "pnpm exec drizzle-kit migrate",
    "db:studio": "pnpm exec drizzle-kit studio",
    "db:push": "pnpm exec drizzle-kit push",
    "db:reset": "pnpm exec drizzle-kit reset",
    "db:seed": "pnpm exec drizzle-kit seed",
    "migrate:better-auth": "ts-node scripts/migrate-to-better-auth.ts",
    "create-admin": "ts-node scripts/migrate-to-better-auth.ts create-admin",
    "postdeploy": "npm run db:migrate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.11.0",
  "dependencies": {
    "@neondatabase/serverless": "^1.0.0",
    "@upstash/qstash": "^2.8.1",
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.34.9",
    "@upstash/workflow": "^0.2.13",
    "axios": "^1.9.0",
    "better-auth": "^1.2.8",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "discord.js": "^14.19.3",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "glob": "^11.0.2",
    "helmet": "^8.1.0",
    "lru-cache": "^11.1.0",
    "resend": "^4.5.1",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.18",
    "@types/express": "^4.17.22",
    "@types/node": "^22.15.18",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.27.0",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
